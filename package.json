    "axios": "^1.1.3",
    "browserify-zlib": "^0.2.0",
    "classnames": "^2.3.1",
    "crypto-browserify": "^3.12.0",
    "howler": "^2.2.3",
    "https-browserify": "^1.0.0",
    "immer": "^9.0.18",
    "less": "^4.1.3",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "os-browserify": "^0.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.0.2",
    "react-refresh": "^0.14.0",
    "react-router-dom": "^6.10.0",
    "react-switch": "^7.0.0",
    "sass": "^1.43.4",
